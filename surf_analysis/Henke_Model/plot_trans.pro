;; 	program to plot filter transmission for henke_data_set;;	TNW  5/93;pro plot_trans,wv,trans,title, plotit=plotit, ytitle=ytitletemptitle = !p.subtitle!p.subtitle = ''if n_params(0) lt 3 then title = ' 'if keyword_set(plotit) then ps2!p.multi = [0,0,2,0, 0]wt = where( wv gt 50 )set_xy,0,0,0,max( [1.0,max(trans(wt))] )!mtitle = '!6' + title!xtitle = ''if keyword_set(ytitle) then !ytitle = ytitle else !ytitle='Transmission'!fancy=4!type=16set_viewport, 0.15, 0.95, 0.58, 0.93plot,wv,trans,thick=2set_xy,0,0,1E-10,fix(max( [1.0,max(trans(wt))] )*10.)/10!mtitle=''!xtitle = '!6Wavelength (Angstroms)'!p.subtitle = temptitleset_viewport, 0.15, 0.95, 0.15, 0.50plot_io,wv,trans,thick=2if keyword_set(plotit) then send2!p.multi = 0set_xy, 0,0,0,0set_viewport,0,0,0,0!fancy=2!type=0!mtitle = title		; for others to usereturnend